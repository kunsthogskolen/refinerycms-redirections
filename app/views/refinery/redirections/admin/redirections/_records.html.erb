<%= render 'refinery/admin/search_header', url: refinery.redirections_admin_redirections_path %>
<div class="pagination_container">
  <% if @redirections.any? %>
  <%= render 'redirections' %>
  <% else %>
    <p>
      <% if searching? %>
        <%= t(:no_results, scope: 'refinery.admin.search') %>
      <% else %>
        <strong>
          <%= t('.no_items_yet') %>
        </strong>
      <% end %>
    </p>
  <% end %>
</div>
